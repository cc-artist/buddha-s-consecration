{
  "version": 2,
  "buildCommand": "cd cyber-buddha-blessing && npm install && npm run build",
  "outputDirectory": "cyber-buddha-blessing/.next",
  "installCommand": "cd cyber-buddha-blessing && npm install",
  "builds": [
    {
      "src": "cyber-buddha-blessing/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}